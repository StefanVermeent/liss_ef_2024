	year = {2022},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/icd.2290},
	keywords = {adaptation, adaptation-based approach, early-life stress, evolutionary-developmental psychology, life history theory},
	pages = {e2290},
