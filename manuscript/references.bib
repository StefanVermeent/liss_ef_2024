@misc{rey_mermet_2024,
	title = {Beyond unity and diversity: {Tasks} used to assess attentional control measure task-specific processes},
	shorttitle = {Beyond {Unity} and {Diversity}},
	url = {https://osf.io/jvbfn},
	doi = {10.31234/osf.io/jvbfn},
	abstract = {Attentional control – also referred to as cognitive control or executive functions – is defined as our ability to supervise and control our thoughts and actions in order to achieve our goals. So far, attentional control has been conceptualized either as a unitary construct or as a heterogeneous construct with diverse (sub-)constructs. Irrespective of the conceptualizations, one common assumption is that attentional control – or its (sub-)constructs – can be measured with different tasks. That is, it is not necessary that all tasks measure the same attentional-control process, but at least two different tasks should measure the same attentional-control process. However, recent research has questioned this assumption using three different lines of research. The first line used an experimental approach, the second investigated the impact of age, and the third used a correlational approach. Until now, these different lines of research have been considered apart. In this review, I integrate them, coming the conclusion that the tasks used to assess attentional control measure task-specific processes. In closing, I address the consequences of this conclusion. In particular, I emphasize the necessity (1) to stop generalizing the results beyond the specific task used in the study and (2) to stop using attentional control to explain group and individual differences.},
	language = {en-us},
	urldate = {2024-12-16},
	publisher = {OSF},
	author = {Rey-Mermet, Alodie},
	month = may,
	year = {2024},
	keywords = {individual differences, executive functions, cognitive control, aging, correlational approach, experimental approach},
	file = {OSF Preprint:C\:\\Users\\Stefan Vermeent\\Zotero\\storage\\N3V8XB7C\\Rey-Mermet - 2024 - Beyond Unity and Diversity Tasks Used to Assess Attentional Control Measure Task-Specific Processes.pdf:application/pdf},
}

@article{muskens_2024,
