@misc{lee_2023,
	title = {Precise individual measures of inhibitory control},
	url = {https://osf.io/rj2bu},
	doi = {10.31234/osf.io/rj2bu},
	abstract = {Inhibitory control is essential to daily function, and its disruptions are considered core elements of many psychiatric disorders. One popular measure of inhibitory control is the congruency effect. However, recent research has reported low reliability of the congruency effect, limiting its use for clinical and basic research questions. Here, we asked whether it is possible to obtain precise individual estimates of the congruency effect. We extensively sampled more than five thousand trials in nine participants across four inhibitory control tasks. With this dataset, made public for the community, we show that precise individual estimates are possible but with substantially higher numbers of trials than are collected in many common tools. Using a combination of datasets and simulations, we demonstrate that extensive sampling is necessary to reveal true individual differences and improve observations from alternative modeling approaches. Extensive sampling provides a promising avenue for characterizing commonalities and differences in inhibitory control.},
	language = {en-us},
	urldate = {2024-05-02},
	publisher = {OSF},
	author = {Lee, Hyejin J. and Smith, Derek Martin and Hauenstein, Clifford and Dworetsky, Ally and Kraus, Brian T. and Dorn, Megan and Nee, Derek and Gratton, Caterina},
	month = oct,
	year = {2023},
	keywords = {congruency effect, executive function, individual differences, Inhibitory control, reliability},
}

@article{akker_2021,
