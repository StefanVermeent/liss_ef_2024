	journal = {Child Maltreatment},
	author = {Roberts, Andrea L. and Sumner, Jennifer A. and Koenen, Karestan C. and Kubzansky, Laura D. and Grodstein, Francine and Rich-Edwards, Janet and Weisskopf, Marc G.},
